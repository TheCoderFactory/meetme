<%= simple_form_for(@meeting) do |f| %>
  <%= f.error_notification %>

  <div class="row">
    <div class="input-field col m12">
      <%= f.input :name %>
    </div>
    <div class="input-field col m12">
      <%= f.input :reason, input_html: { class: "materialize-textarea" } %>
    </div>
  </div>
  <div class="row">
    <div class="input-field col m6">
      <%= f.input :street %>
    </div>
    <div class="input-field col m6">
      <%= f.input :suburb %>
    </div>
  </div>
  <div class="row">
    <div class="input-field col m6">
      <%= f.input :postcode %>
    </div>
    <div class="input-field col m6">

      <%= f.input :state, label: false, collection: ['NSW', 'ACT', 'VIC', 'WA', 'SA', 'NT', 'QLD', 'TAS'], input_html: { class: 'browser-default'}, prompt: 'State' %>
    </div>
  </div>
  <div class="row">
    <div class="input-field col m12">
     
      <%= f.input :date, as: :string, input_html: { class: "datepicker" }%>
    </div>
  </div>
